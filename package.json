{
  "name": "hmb-docs-template",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "gen:headers": "ts-node tools/extract-headers.ts",
    "build": "npm run gen:headers && next build",
    "dev": "npm run gen:headers && next dev",
    "start": "next start",
    "postinstall": "fumadocs-mdx",
    "preview": "opennextjs-cloudflare build && opennextjs-cloudflare preview",
    "deploy": "opennextjs-cloudflare build && opennextjs-cloudflare deploy",
    "cf-typegen": "wrangler types --env-interface CloudflareEnv cloudflare-env.d.ts",
    "export-static": "EXPORT_STATIC=true node scripts/build-static-standalone.js",
    "crawl-static": "node scripts/crawl-static-html.js"
  },
  "dependencies": {
    "@opennextjs/cloudflare": "^1.9.1",
    "class-variance-authority": "^0.7.1",
    "fumadocs-core": "15.8.1",
    "fumadocs-mdx": "^12.0.3",
    "fumadocs-ui": "15.8.1",
    "lucide-react": "^0.544.0",
    "next": "15.5.4",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "tailwind-merge": "^3.3.1"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.13",
    "@types/mdx": "^2.0.13",
    "@types/node": "^24.10.0",
    "@types/react": "^19.1.14",
    "@types/react-dom": "^19.1.9",
    "archiver": "^7.0.1",
    "postcss": "^8.5.6",
    "puppeteer": "^24.27.0",
    "tailwindcss": "^4.1.13",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3",
    "wrangler": "^4.42.0"
  }
}
